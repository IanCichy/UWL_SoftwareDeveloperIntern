public class Application
{
    public String stuid, appID, firstName, lastName, email, phone, major, year, work, credits, gpa, hallID;
    
    public Application(String stu, String first, String last, String ema, String ph, String ma, String yr, String wo, String cr, String gp)
	{
        stuid = stu;
        firstName = first;
        lastName = last;
        email = ema;
        phone = ph;
        major = ma;
        year = yr;
        work = wo;
        credits = cr;
        gpa = gp;
	}

    public Application(String hall, String app, String stu, String first, String last, String ema, String ph, String ma, String yr, String wo, String cr, String gp)
    {
        hallID = hall;
        appID = app;
        stuid = stu;
        firstName = first;
        lastName = last;
        email = ema;
        phone = ph;
        major = ma;
        year = yr;
        work = wo;
        credits = cr;
        gpa = gp;
    }
}
